        <messages/>
        <name>Validar campo de visitantes</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    var myVal = g_form.getValue('qtd_visitantes');

    var matchPattern = /([\d])/g;
    var myResult = matchPattern.test(myVal);

    if (!myResult) {
        g_form.clearValue('qtd_visitantes');
        g_form.showFieldMsg('qtd_visitantes', 'Digite somente números', 'error');
    }

}]]></script>
